{
  "version": 2,
  "builds": [
    {
      "src": "api/yt_summ_gradio.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/yt_summ_gradio.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1",
      "continue": true
    }
  ]
} 

  